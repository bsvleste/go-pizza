{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@routes/*":["./src/routes/*"],
      "@screens/*":["./src/screens/*"],
      "@storage/*":["./src/storage/*"],
      "@utils/*":["./src/utils/*"],
      "@hooks/*":["./src/hooks/*"],
      "@config/*":["./src/config/*"],
    }
  }
}
